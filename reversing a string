#include<stdio.h>
#include<string.h>
int main(void)
// program to reverse a string
{
   char  str[400];
   int i,n;
   printf(" enter the string to be reversed \n");
   gets(str);
   n=strlen(str);
   char kk[400];
   strcpy(kk,str);
   for(i=0;i<n;i++)
    {
       kk[i+n-1] = str[i];
    }
    printf("reversed string is \n");
  puts(str);
}
